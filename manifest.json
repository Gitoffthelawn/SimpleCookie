{
  "manifest_version": 3,
  "name": "SimpleCookie",
  "version": "2.1",
  "description": "A minimalist yet efficient cookie manager for Firefox",
  "permissions": [
    "browsingData",
    "cookies",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "options_ui": {
    "page": "settings.html"
  }
}