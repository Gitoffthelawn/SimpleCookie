{
  "manifest_version": 2,
  "name": "SimpleCookie",
  "version": "1.3",
  "description": "A minimalist yet efficient cookie manager for Firefox",
  "permissions": [
    "browsingData",
    "cookies",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  }
}